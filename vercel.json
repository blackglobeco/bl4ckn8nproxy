{
  "version": 2,
  "builds": [{ "src": "api/index.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js",
      "headers": {
        "X-Frame-Options": "",
        "Content-Security-Policy": "",
        "Cross-Origin-Embedder-Policy": "",
        "Cross-Origin-Opener-Policy": "",
        "Cross-Origin-Resource-Policy": "",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
